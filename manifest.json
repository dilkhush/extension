{
  "manifest_version": 2,
  "name": "Share Point",
  "version": "1.0",
  "description": "My first Chrome extension.",
  "browser_action": {
    "default_icon": "icon.png"
  },
  "web_accessible_resources": [
    "index.css",
    "init.js",
    "TextHighlighter.js",
    "twitter.png",
    "email.png",
    "social_network.js",
    "jquery-2.1.1.min.js"
  ],
  "content_scripts": [
    {
      "matches" : ["<all_urls>"],
      "js" : ["init.js"]
    }
  ]
}